  <Target Name="MoveDllsToLibs" AfterTargets="Build">
    <!-- Выбираем все DLL в bin\Debug или bin\Release -->
    <ItemGroup>
      <AllDlls Include="$(OutputPath)*.dll" Exclude="$(OutputPath)$(AssemblyName).dll" />
    </ItemGroup>
    <!-- Копируем в папку libs -->
    <Copy SourceFiles="@(AllDlls)" DestinationFolder="$(OutputPath)libs\" SkipUnchangedFiles="true" />
    <!-- Удаляем из корня bin, чтобы остались только в libs -->
    <Delete Files="@(AllDlls)" />
  </Target>
  <Target Name="MoveXmlsToXmlsFolder" AfterTargets="Build">
    <!-- Выбираем все xml рядом с exe -->
    <ItemGroup>
      <AllXmls Include="$(OutputPath)*.xml" Exclude="$(OutputPath)App.config" />
    </ItemGroup>
    <!-- Копируем в подпапку .xmls -->
    <Copy SourceFiles="@(AllXmls)" DestinationFolder="$(OutputPath)xmls\" SkipUnchangedFiles="true" />
    <!-- Удаляем из корня bin -->
    <Delete Files="@(AllXmls)" />
  </Target>
